{
  "name": "@appbaseio/reactivesearch",
  "version": "0.5.21",
  "description": "A React UI components library for building search experiences",
  "main": "lib/app.js",
  "scripts": {
    "lint": "esw app --color --watch",
    "test": "react-scripts test --env=jsdom",
    "webpack-server": "webpack-dev-server --hot --inline --progress --colors --port 8000",
    "web-server": "http-server -p 3000 .",
    "start": "concurrently \"npm run webpack-server\" \"gulp watch\" ",
    "build": "cross-env NODE_ENV=production CHOOSE_CONFIG=EXAMPLES webpack -p --progress && gulp",
    "build:lib": "gulp && cross-env NODE_ENV=production CHOOSE_CONFIG=LIB babel app --out-dir lib",
    "build:umd": "cross-env NODE_ENV=production webpack --config webpack.config.umd.js app/app.js umd/ReactiveSearch.js",
    "build-bro": "browserify -t [ babelify --presets [ es2015 stage-0 react ] ] main.js -o dist/main.bundle.js",
    "storybook": "start-storybook -p 9009",
    "build-storybook": "build-storybook -o playground",
    "build:all": "npm run build && npm run build:lib && npm run build:umd && npm run build-storybook"
  },
  "files": [
    "lib/",
    "dist/",
    "umd/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appbaseio/reactivesearch.git"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/appbaseio/reactivesearch/issues"
  },
  "homepage": "https://github.com/appbaseio/reactivesearch#readme",
  "dependencies": {
    "@appbaseio/reactivemaps": "git+https://github.com/rbajaniya/reactivemaps-dev.git",
    "classnames": "^2.2.5",
    "jquery": "^3.1.1",
    "lodash": "^4.15.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.1",
    "rc-slider": "^5.4.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-select": "^1.0.0-rc.3",
    "react-stars": "^2.1.0",
    "sass-loader": "^6.0.3",
    "shebang-loader": "0.0.1"
  },
  "devDependencies": {
    "@appbaseio/reactivemaps-manual": "1.0.0-alpha",
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "@kadira/storybook-addon-options": "^1.0.1",
    "@kadira/storybook-addons": "^1.6.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.9.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "concurrently": "^2.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.4",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "^3.0.0",
    "file-loader": "^0.9.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-file-include": "^1.0.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.3.1",
    "gulp-uglify": "^1.5.3",
    "json-loader": "^0.5.4",
    "lodash-webpack-plugin": "^0.10.6",
    "materialize-css": "^0.98.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "shebang-loader": "0.0.1",
    "storybook-readme": "^1.2.0",
    "style-loader": "^0.14.1",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.16.0"
  }
}
